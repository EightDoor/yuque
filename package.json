{
  "name": "vite-project",
  "version": "0.0.0",
  "main": "./electron/main.js",
  "scripts": {
    "z": "concurrently  \"npm run start\" \"npm run dev\"",
    "start": "wait-on -c waitOnConfig.json http-get://localhost:9999/ -v && electron ./electron",
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "serve": "vite preview",
    "lint": "yarn prettier --write",
    "pack": "electron-builder --dir",
    "dist:all": "electron-builder -mwl",
    "dist:mac": "electron-builder --mac",
    "dist:win": "electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps",
    "dist:linux": "electron-builder --linux deb tar.xz AppImage"
  },
  "type": "commonjs",
  "author": "周凯 <851708184@qq.com>",
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "electron-is-dev": "^2.0.0",
    "element-plus": "^1.0.2-beta.65",
    "localforage": "^1.9.0",
    "lodash": "^4.17.21",
    "md-editor-v3": "^1.3.3",
    "qs": "^6.10.1",
    "vue": "^3.2.1",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2",
    "yuque-auth": "^1.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.4.10",
    "@types/prismjs": "^1.16.6",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.1.5",
    "concurrently": "^6.2.0",
    "debugout.js": "^1.0.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.15.1",
    "less": "^4.1.1",
    "prettier": "2.3.2",
    "sass": "^1.37.5",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vite-plugin-style-import": "^1.0.1",
    "vue-tsc": "^0.2.2",
    "wait-on": "^6.0.0"
  },
  "linux": {
    "icon": "dist/icons"
  }
}